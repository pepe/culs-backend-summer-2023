{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Logs",
        "Args": {
          "hostnameOrAddress": "127.0.0.1",
          "port": "11200",
          "transportType": "Udp"
        }
      }
    ],
    "Properties": {
      "Application": "LogsApplication"
    }
  }
}
